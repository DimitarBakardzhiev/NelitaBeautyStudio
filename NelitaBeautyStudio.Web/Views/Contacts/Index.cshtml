@using NelitaBeautyStudio.Web.ViewModels
@model IEnumerable<ContactViewModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Контакти</h2>

@if (Model.Count() == 0)
{
    <h3>Няма въведени контакти</h3>
}
else
{
    <table class="table table-striped table-hover">
        <tbody>
            @foreach (var contact in Model)
            {
                <tr>
                    <td>@contact.Type</td>
                    <td>@contact.Value</td>
                </tr>
            }
        </tbody>
    </table>
}